# Copy this file to .env and fill in your actual values
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# AI Service API Keys
EXPO_PUBLIC_GEMINI_API_KEY=your_gemini_api_key_here
EXPO_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here

# Google Cloud Service Account JSON (for Vision API)
EXPO_PUBLIC_GOOGLE_APPLICATION_CREDENTIALS_JSON={"type": "service_account", "project_id": "your_project_id", "private_key_id": "your_key_id", "private_key": "your_private_key", "client_email": "your_service_account_email", "client_id": "your_client_id", "auth_uri": "https://accounts.google.com/o/oauth2/auth", "token_uri": "https://oauth2.googleapis.com/token", "auth_provider_x509_cert_url": "https://www.googleapis.com/oauth2/v1/certs", "client_x509_cert_url": "your_cert_url", "universe_domain": "googleapis.com"}

# Supabase Service Key (for advanced operations)
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# Google Vision API Key (separate from service account credentials)
EXPO_PUBLIC_GOOGLE_VISION_API_KEY=your_google_vision_api_key_here